kind: ClusterRoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: app-metrics-operator
subjects:
- kind: ServiceAccount
  name: app-metrics-operator
  namespace: {{ mobile_app_metrics_namespace }}
roleRef:
  kind: ClusterRole
  name: app-metrics-operator
  apiGroup: rbac.authorization.k8s.io